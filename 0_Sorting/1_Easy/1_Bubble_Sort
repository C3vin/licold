public static void bubbleSort(int [] a) {
	    for (int i = 0; i < a.length; i++) {
	        int flag = 0;
	        for (int j = 1; j < a.length - i; j++) {
	            if ((a[j - 1])  > a[j] ) {
	        	    int temp = a[j];
	                a[j] = a[j - 1];
	                a[j - 1] = temp;
	                flag = 1;
	            }
	        }
	        if (flag == 0) {
	            return;
	        }
	    }
	}
